{
  "name": "elements/process-manager-bundle",
  "type": "pimcore-bundle",
  "description": "The Process Manager allows you to manage (define,execute...) arbitrary processes/commands in the Pimcore backend.",
  "require": {
    "pimcore/pimcore": ">=6.0",
    "mtdowling/cron-expression": "^1.1.0"
  },
  "autoload": {
    "psr-4": {
        "Elements\\Bundle\\ProcessManagerBundle\\": "src"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "Pimcore\\Composer::executeBundleMigrationsUp"
    ]
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Elements\\Bundle\\ProcessManagerBundle\\ElementsProcessManagerBundle"
      ]
    }
  }
}
